{
  "name": "template-service-clean-arch-nest",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "commit": "cz",
    "build": "rm -rf .build && nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\"",
    "lint:fix": "yarn lint --fix",
    "watch": "nest start --debug 0.0.0.0:9229 --watch",
    "dev": "nest start",
    "dev:repl": "nest start -- --entryFile repl",
    "db:studio": "prisma studio --schema './database/schema.prisma'",
    "db:migrations": "prisma migrate dev --schema './database/schema.prisma' --create-only --name",
    "db:migrate": "prisma migrate dev --schema './database/schema.prisma'",
    "prisma:sync": "prisma generate --schema './database/schema.prisma'",
    "prepare": "husky install",
    "prisma:prod:deploy": "prisma migrate deploy --schema './database/schema.prisma'",
    "prestart:prod": "npm run prisma:prod:deploy",
    "semantic-release": "semantic-release",
    "start:prod": "node .build/server",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json"
  },
  "dependencies": {
    "@nestjs/common": "^10.1.0",
    "@nestjs/core": "^10.1.0",
    "@nestjs/jwt": "^10.1.1",
    "@nestjs/mapped-types": "^2.0.2",
    "@nestjs/passport": "^10.0.2",
    "@nestjs/platform-express": "^10.1.0",
    "@nestjs/swagger": "^7.1.13",
    "@nestjs/terminus": "^10.1.1",
    "@opentelemetry/context-async-hooks": "^1.17.0",
    "@opentelemetry/exporter-prometheus": "^0.43.0",
    "@opentelemetry/exporter-trace-otlp-http": "^0.43.0",
    "@opentelemetry/instrumentation-express": "^0.33.1",
    "@opentelemetry/instrumentation-nestjs-core": "^0.33.1",
    "@opentelemetry/instrumentation-pino": "^0.34.1",
    "@opentelemetry/propagator-b3": "^1.17.0",
    "@opentelemetry/propagator-jaeger": "^1.17.0",
    "@opentelemetry/sdk-node": "^0.43.0",
    "@opentelemetry/sdk-trace-base": "^1.17.0",
    "@prisma/client": "^5.0.0",
    "@prisma/instrumentation": "^5.4.1",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "cookie-parser": "^1.4.6",
    "crypto-js": "^4.1.1",
    "dotenv": "^16.3.1",
    "env-var": "^7.3.1",
    "jsonwebtoken": "^9.0.2",
    "nestjs-otel": "^5.1.5",
    "nestjs-pino": "^3.5.0",
    "passport": "^0.6.0",
    "passport-jwt": "^4.0.1",
    "pino": "^8.15.6",
    "pino-http": "^8.5.0",
    "pino-loki": "^2.1.3",
    "pino-pretty": "^10.2.3",
    "randomstring": "^1.3.0",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.2.0",
    "short-uuid": "^4.2.2",
    "swagger-ui-express": "^5.0.0"
  },
  "devDependencies": {
    "@nestjs/cli": "^10.1.9",
    "@nestjs/schematics": "^10.0.1",
    "@nestjs/testing": "^10.1.0",
    "@types/crypto-js": "^4.1.2",
    "@types/express": "^4.17.13",
    "@types/hasha": "^3.0.1",
    "@types/jest": "29.5.3",
    "@types/jsonwebtoken": "^9.0.3",
    "@types/ms": "^0.7.32",
    "@types/node": "20.4.2",
    "@types/passport-jwt": "^3.0.10",
    "@types/passport-local": "^1.0.36",
    "@types/randomstring": "^1.1.9",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^6.1.0",
    "@typescript-eslint/parser": "^6.1.0",
    "cz-conventional-changelog": "3.3.0",
    "eslint": "^8.45.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-prettier": "^5.0.0",
    "husky": "^8.0.3",
    "jest": "29.6.1",
    "lint-staged": "^13.2.3",
    "prettier": "^3.0.0",
    "prisma": "^5.0.0",
    "semantic-release": "^21.0.7",
    "source-map-support": "^0.5.20",
    "supertest": "^6.1.3",
    "ts-jest": "29.1.1",
    "ts-loader": "^9.4.4",
    "ts-node": "^10.0.0",
    "tsconfig-paths": "4.2.0",
    "typescript": "^5.1.6"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
